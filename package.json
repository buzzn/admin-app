    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "bootstrap-loader": "2.0.0-beta.20",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "chai-spies": "^0.7.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "d3": "^4.4.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.0",
