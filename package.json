  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 10"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "plugins": [
      "react",
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "max-len": [
        2,
        120,
        2,
        {
          "ignoreUrls": true,
          "ignoreComments": false
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "multiline": true
        }
      ],
      "react/jsx-uses-vars": 2,
      "react/jsx-uses-react": 2
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "modules": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "octalLiterals": true,
        "regexUFlag": true,
        "regexYFlag": true,
        "restParams": true,
        "spread": true,
        "superInFunctions": true,
        "templateStrings": true,
        "unicodeCodePointEscapes": true,
        "globalReturn": true,
        "jsx": true,
        "prefer-const": true
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/app/__mocks__/styleMock.js"
    }
  }
}
