{
  "name": "powergiver",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require babel-polyfill --require tests-setup.js --require ignore-styles $(find . -name '*.tests.js' ! -ipath '*node_modules*')",
    "dev-server": "webpack-dashboard -- webpack-dev-server --progress --inline --hot --port=2999",
    "build": "rimraf build && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start": "./node_modules/babel-cli/bin/babel-node.js server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.0",
